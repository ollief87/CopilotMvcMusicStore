@model IEnumerable<CopilotMvcMusicStore.Web.Models.Genre>
@{
    ViewData["Title"] = "Store";
}
<h3>Browse Genres</h3>
<ul id="genres">
    @foreach (var genre in Model)
    {
        <li>
            <a href="@Url.Action("Browse", "Store", new { genre = genre.Name })">@genre.Name</a>
        </li>
    }
</ul>